{
  "compilerOptions": {
    "moduleResolution": "node",
    "strict": true,
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es5",
    "allowJs": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "types": [
      "node",
      "express"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
